<?xml version='1.0' encoding='utf-8'?>
<widget id="co.femr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>vue-cordova template</name>
    <description>
        Template vue-cli cordova app
    </description>
    <author email="fabiomoreno@outlook.com" href="http://cordova.io">
        Fabio E. Moreno
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-battery-status" spec="^2.0.2" />
    <engine name="android" spec="^7.1.0" />
</widget>
